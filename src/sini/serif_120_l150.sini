[HeaderAndFooter]
; $A - author $C:chapter $T:Title $Series
HeaderSource=$AT
FooterSource=$C
HeaderWidth=70
FooterWidth=70
HeaderShow=1
FooterShow=1
; shift from top
HeaderTextShift=5
; 0: none, 1:solid, 2:dashed, 3:double 4;dotted
HeaderLineStyle=3
HeaderLineColor=127
; indent from left
HeaderLineLeft=5
HeaderLineRight=5
; from top
HeaderLineShift=28
HeaderLineWidth=1
FooterTextShift=10
FooterLineStyle=3
FooterLineColor=127
FooterLineLeft=5
FooterLineRight=5
FooterLineShift=1
FooterLineWidth=1

[TitleAndAutorOrder]
; 0: (ser)+Title, 1: Tilte +(ser)
TitleOrder=0
; 0 Firstname Lastname 1: Lastname Firstname
AuthorOrder=0
; 0: no initials 1: use initials  
Initials=0

[References]
; 0: none 1:superscript
RefSuperScript=0
; underline ref 0: none, 1:solid, 2:dashed, 3:double 4;dotted
RefUnderLine=2
; 0 as is 1 no brackets 2 brackets ([ref])
RefStyle=0
; in Reference in Notes underline ref 0: none, 1:solid, 2:dashed, 3:double 4;dotted
RefNUnderLine=2
; in Notes 0 as is 1 no brackets 2 brackets ([ref])
RefNStyle=0

[General]
; 0 do not modify size of cover picture 1: zoom to screen
CoverImageAdjust=1
; make new page at subtitkles
PageAtSubtitles=0
; use translit for TOC
Translit=0

[SaveJob]
; no meaning in console version
AutoSaveOnOK=1
