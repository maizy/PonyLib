;-------STYLES FOR FB2LRF----------------------
;[InternalFonts] are not used in console version
[InternalFonts]
0=Dutch801 Rm BT Roman
1=Swis721 BT Roman
2=Courier 10 Pitch Win95BT

[Page]
;Margins for page
Top=5
Left=5
Right=5
Bottom=5
HeaderHeight=0
; distance from header to text area
HeaderSeparation=0
FooterHeight=0
FooterSeparation=0

[MainText]
;Full font name that will be included to lrf
FontName=Swis721 BT Roman
; Fontsize (pt) multiplied by 10
FontSizex10=120
; isBold=1: bold isBold=0:normal 
IsBold=0
; isItalic=1: italic isItalic=0:normal 
IsItalic=0
; if Font is external (taken from the external font file(see below)) isExternalFont=0
IsExternalFont=-1
; full path to external font file
ExternalFontFile=
MarginLeft=0
MarginRight=0
; -1: not used 
MarginTop=-1
MarginBottom=-1
ParIdent=4
ParSkip=0
LineSpace=150
WordSpace=20
; BlockAlign 0-justify 1-center 2 right
BlockAlign=0
; BlockRule 0: "horz-fixed", 1 "horz-adjustable", 2 "vert-fixed", 3 "vert-adjustable", 4 "block-fixed", 5 "block-adjustable"
BlockRule=0
AuthorIdent=15
;00 - regular 01 bold 10 italic
AuthorBoldItalic=17

